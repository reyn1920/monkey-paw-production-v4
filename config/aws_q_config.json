{
  "amazon_q": {
    "enabled": true,
    "region": "us-east-1",
    "profile": "default",
    "features": {
      "code_completion": true,
      "code_explanation": true,
      "code_generation": true,
      "security_scanning": true,
      "performance_optimization": true
    },
    "integration": {
      "vscode": true,
