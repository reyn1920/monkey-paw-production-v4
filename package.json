{
  "name": "stackbuilder-pro",
  "version": "1.0.0",
  "description": "Professional AI Development Stack Builder - Multi-Platform Integration",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3002 --host 0.0.0.0",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3002",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "health:8000": "node scripts/health-service-8000.js",
    "health:4000": "node scripts/health-service-4000.js",
    "health:both": "concurrently -n H8000,H4000 \"npm run health:8000\" \"npm run health:4000\"",
    "start:all": "concurrently -n APP,H8000,H4000 \"npm run dev\" \"npm run health:8000\" \"npm run health:4000\"",
    "puppeteer:test": "node scripts/puppeteer-automation.js",
    "content:generate": "node automation/content-generator.js generate",
    "content:batch": "node automation/content-generator.js batch",
    "video:scan": "node automation/video-pipeline.js scan",
    "video:generate": "node automation/video-pipeline.js generate",
    "video:batch": "node automation/video-pipeline.js batch",
    "video:status": "node automation/video-pipeline.js status",
    "business:scan": "node automation/business-automation-system.js scan",
    "business:start": "node automation/business-automation-system.js start",
    "business:status": "node automation/business-automation-system.js status",
    "audit:system": "node scripts/system-integration-audit.js",
    "automation:start": "node automation/automation-orchestrator.js start",
    "automation:stop": "node automation/automation-orchestrator.js stop",
    "automation:status": "node automation/automation-orchestrator.js status",
    "automation:execute": "node automation/automation-orchestrator.js execute",
    "verify": "node automation/verification-system.js verify",
    "monitor": "node automation/verification-system.js monitor",
    "monitor:stop": "node automation/verification-system.js stop",
    "monitor:metrics": "node automation/verification-system.js metrics",
    "setup": "npm install && npm run verify",
    "dev:full": "concurrently -n APP,H8000,H4000,AUTO \"npm run dev\" \"npm run health:8000\" \"npm run health:4000\" \"npm run automation:start\"",
    "production:start": "npm run build && npm run start:all",
    "production:full": "npm run build && concurrently -n APP,H8000,H4000,AUTO \"npm run preview\" \"npm run health:8000\" \"npm run health:4000\" \"npm run automation:start\"",
    "full:rebuild": "bash scripts/FULL_REBUILD.sh",
    "run:everything": "bash scripts/RUN_EVERYTHING.sh",
    "ports:guard": "bash scripts/ports_guard.sh",
    "ports:monitor": "bash scripts/monitor_ports.sh",
    "ports:resolve": "bash scripts/resolve_port_conflicts.sh",
    "bg:remove": "python3 scripts/bg_remove_stub.py",
    "analytics:log": "python3 analytics/log_event.py",
    "analytics:summary": "python3 analytics/log_event.py summary",
    "analytics:export": "python3 analytics/log_event.py export",
    "go:live": "bash scripts/go_live_enable.sh",
    "go:test": "bash scripts/go_live_disable.sh",
    "agents:load": "bash scripts/launchagents_load.sh",
    "agents:unload": "bash scripts/launchagents_unload.sh",
    "right:perspective:check": "bash scripts/check_right_perspective.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@tanstack/react-query": "^4.24.6",
    "axios": "^1.3.4",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "firebase": "^12.3.0",
    "framer-motion": "^10.0.0",
    "lucide-react": "^0.312.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^6.8.1",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.20.6",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/ui": "^0.28.5",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jsdom": "^21.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "puppeteer": "^19.7.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.28.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "ai-development",
    "stack-builder",
    "multi-platform",
    "cursor",
    "bolt-diy",
    "windsurf",
    "vscode",
    "firebase",
    "trae-ai",
    "react",
    "typescript",
    "vite"
  ],
  "author": "Thomas Brian Reynolds",
  "license": "MIT"
}
